        <mxCell id="kLhNIvPnXdjAd_poE6zR-40" value="Получение информации о записанных к врачу пациентах&lt;br&gt;[HTTPS, LAN]" style="edgeLabel;html=1;align=center;verticalAlign=middle;resizable=0;points=[];" vertex="1" connectable="0" parent="kLhNIvPnXdjAd_poE6zR-39">
